  "scripts": {
    "build": "run-s clean test lint format build:*",
    "build:rollup": "rollup -c --configPlugin typescript",
    "dev": "tsc -w --noEmit",
    "clean": "del \"packages/*/dist\"",
    "format": "run-s format:*",
    "format:sort-package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "prettier --write .",
    "lint": "eslint --ext .ts,.tsx --fix .",
    "test": "jest --passWithNoTests"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "import",
      "react-hooks"
    ],
    "rules": {
      "no-var": "error",
      "padding-line-between-statements": [
        "warn",
        {
          "blankLine": "always",
          "prev": "import",
          "next": "*"
        },
        {
          "blankLine": "any",
          "prev": "import",
          "next": "import"
        },
        {
          "blankLine": "always",
          "prev": "*",
          "next": "export"
        },
        {
          "blankLine": "any",
          "prev": "export",
          "next": "export"
        },
        {
          "blankLine": "always",
          "prev": "*",
          "next": [
            "class",
            "function",
            "block",
            "block-like",
            "multiline-expression",
            "multiline-const",
            "multiline-let"
          ]
        },
        {
          "blankLine": "always",
          "prev": [
            "class",
            "function",
            "block",
            "block-like",
            "multiline-expression",
            "multiline-const",
            "multiline-let"
          ],
          "next": "*"
        },
        {
          "blankLine": "never",
          "prev": "*",
          "next": [
            "case",
            "default"
          ]
        },
        {
          "blankLine": "never",
          "prev": [
            "case",
            "default"
          ],
          "next": "*"
        }
      ],
      "import/no-absolute-path": "error",
      "import/no-dynamic-require": "error",
      "import/no-self-import": "error",
      "import/no-useless-path-segments": "error",
      "import/no-webpack-loader-syntax": "error",
      "import/order": [
        "error",
        {
          "alphabetize": {
            "order": "asc"
          }
        }
      ],
      "react-hooks/exhaustive-deps": "warn",
      "react-hooks/rules-of-hooks": "error"
    },
    "ignorePatterns": [
      "**/dist/**"
    ]
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true,
        "babelConfig": {
          "babelrc": false,
          "configFile": false,
          "presets": [
            [
              "@babel/preset-env",
              {
                "modules": "commonjs",
                "targets": {
                  "node": "current"
                }
              }
            ],
            [
              "@babel/preset-react",
              {
                "runtime": "automatic"
              }
            ]
          ],
          "plugins": [
            [
              "babel-plugin-import-meta-url"
            ]
          ]
        }
      }
    },
    "moduleNameMapper": {
      "^@mo36924/([^/]+)/?": "<rootDir>/packages/$1/src/"
    },
    "preset": "ts-jest",
    "snapshotSerializers": [
      "jest-snapshot-serializer-babel",
      "jest-snapshot-serializer-graphql",
      "jest-snapshot-serializer-preact",
      "jest-snapshot-serializer-prettier",
      "jest-snapshot-serializer-raw",
      "jest-snapshot-serializer-sql"
    ],
    "testTimeout": 120000
  },
